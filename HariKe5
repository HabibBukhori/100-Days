package Hari5;

public class DayFive {

  public static void main(String[] args) {
  
  // Hari ke5 ini saya akan melanjutkan
  // operator aritmatika 
  // yaitu pengurangan( - ) dan pembagian( / )
  
  int a = 45;
  int b = 9;
  int c = a + b;
  System.out.println(a-c);
  System.out.println(c/b);
  
  // Yang akan tampil dari codingan di atas adalah
  // a - c itu yang akan tampil adalah -9,dan
  // c / b itu yang akan tambil adalah 6
  // baiklah itulah tadi hasil codingan saya
  // hari ini, maaf jika ada kesalahan saya 
  // sebagai adek mentor kakak. terima kasih :)
  

  }

}
